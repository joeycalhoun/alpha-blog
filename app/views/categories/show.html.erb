<h1 align="center">Articles Associated With <%= @category.name %></h1>

<div align="center">
    <% if logged_in? and current_user.admin? %>
        <div class="row">
            <%= link_to "Edit Category", edit_category_path(@category) %>
        </div>
    <% end %>
    <%= will_paginate @category_articles%>
</div>  

<%= render 'articles/article', obj: @category_articles %>

<div align="center">
    <%= will_paginate @category_articles %>
</div>   